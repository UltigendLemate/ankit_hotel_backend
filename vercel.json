{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        { "for": "*", "key": "Access-Control-Allow-Origin", "value": "https://ankit-hotel.vercel.app/" } 
      ]
  }